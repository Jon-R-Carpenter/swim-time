@model TheSwimTimeSite.Models.ViewModels.CreateEditTermViewModel


@{
    ViewBag.Title = "editTerm";
}
<!DOCTYPE html>
<html>
<head>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />;
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/JQuery1.6.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.16.custom.min.js")" type="text/javascript"></script>

    <script src="https://code.jquery.com/jquery-1.10.2.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            ('.datepicker').datepicker();
        })
    
    </script>
</head>
<body>
    @Html.BeginForm(){ 
    @Html.ValidationSummary(true)
    <div id="visitBackground">
        <fieldset style="margin-top:-420px;">
            @Html.HiddenFor(model => model.Creating.TermID)
            <table>
                <tr>
                    <td align="left">
                        @Html.LabelFor(model => model.Creating.Quarter)
                        @Html.DropDownListFor(m => m.Creating.Quarter, Model.QuartersList, "--")
                        @Html.ValidationMessageFor(model => model.Creating.Quarter)
                    </td>

                </tr>
                <tr>
                    <td align="left">
                        @Html.LabelFor(model => model.Creating.Year)
                        @Html.EditorFor(model => model.Creating.Year)
                        @Html.ValidationMessageFor(model => model.Creating.Year)
                    </td>
                </tr>
                <tr>
                    <td align="left">
                        @Html.LabelFor(model => model.Creating.Start)
                        @Html.TextBoxFor(model => model.Creating.Start,
                    new { @class = "datepicker" })
                        @Html.ValidationMessageFor(model => model.Creating.Start)
                    </td>
                </tr>
                <tr>
                    <td align="left">
                        @Html.LabelFor(model => model.Creating.Endd)
                        @Html.TextBoxFor(model => model.Creating.Endd,
                    new { @class = "datepicker" })
                        @Html.ValidationMessageFor(model => model.Creating.Endd)
                    </td>
                </tr>
                <tr>
                    <td><input type="submit" value="Create" /></td>
                </tr>
            </table>
        </fieldset>
        <br />
        @Html.ActionLink("Terms", "Terms", null, new { @class = "button" })
    </div>
    
    
    
    
    }
</body>
</html>


